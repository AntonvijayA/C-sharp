using System;

namespace ConsoleApp10
{
    internal class Program
    {
        static void Main(string[] args)
        {
    
      
                string transactionID = "TXN123ABC456";       // 12-character alphanumeric
                decimal amount = 1000000.75M;               // Amount in INR
                bool isInternational = true;                // Transaction outside India
                decimal customerRating = 4.75M;             // Rating between 0.0 and 5.0
                DateTime transactionTimestamp = DateTime.Now; // Current date and time
                int rewardPoints = 500000;                  // Whole number, up to millions

                Console.WriteLine("Transaction ID: " + transactionID);
                Console.WriteLine("Amount: â‚¹" + amount);
                Console.WriteLine("Is International: " + isInternational);
                Console.WriteLine("Customer Rating: " + customerRating);
                Console.WriteLine("Transaction Timestamp: " + transactionTimestamp);
                Console.WriteLine("Reward Points: " + rewardPoints);
            }
        }

    }
